{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES5", "DOM"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "allowJs": true,
    // "moduleResolution": "Node"
    // "module": "commonjs",
    // "moduleResolution": "node",
    // "preserveValueImports": false
    "baseUrl": "cypress/e2e",
    //cypress\e2e\pages\Accounts\LoginPage.ts
    "paths": {
      "@accountPages/*": ["./pages/Accounts/*"],
      "@checkoutPages/*": ["./pages/Checkout/*"],
      "@categoriesPages/*": ["./pages/Categories/*"],
      "@navbarPages/*": ["./pages/Navbar/*"],
      "@productPages/*": ["./pages/Products/*"]
    }
  },
  "include": ["**/*.ts"]
}
